    <event eventtype="8" enumb="75">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// End catching the GUI draws and draw the postprocessing surface
// end catching
surface_reset_target();

// Begin drawing the postprocessing surface
var _winw = window_get_width();
var _winh = window_get_height();

if (global.postprocess)
{
    /// draw with filters applied
    
    /// Old method : not so efficient
    if (global.postprocessUnpotimized)
    {
        // background
        draw_clear(0);
        
        var _surfw = surface_get_width(global.surfScreen);
        var _surfh = surface_get_height(global.surfScreen);
        
        var _noise      = sprite_get_texture(texBluenoise, 0);
        var _rainbow    = sprite_get_texture(texRainbowTable, 0);
        
        // CRT filter : make temp. surface to hold the resulting CRT'd screen
        texture_set_interpolation(true);
        var _crtsurf = surface_create(_surfw, _surfh);
        
        shader_set(shd_postprocessing);
            // texture_set_stage(shader_get_sampler_index(shd_postprocessing, "uTexDownscaledScreen"), surface_get_texture(_smallsurf));
            texture_set_stage(shader_get_sampler_index(shd_postprocessing, "uTexBluenoise"), _noise);
            texture_set_stage(shader_get_sampler_index(shd_postprocessing, "uTexRainbow"), _rainbow);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing, "uTime"), current_time * 0.001);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing, "uScreenTexelSize"), 1 / _surfw, 1 / _surfh);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing, "uBluenoiseTexelSize"), texture_get_texel_width(_noise), texture_get_texel_height(_noise));
            
            // animate transition
            if (transitionIsHappening)
            {
                if (transitionIsFadeout)
                {
                    var _screensqueezeamp = 0.8;
                    var _transitioninterp = interp_weight(transitionCtr, transitionTime, 2.0, 1.5);
                    // _transitioninterp = 1.0 - _transitioninterp;
                    var _screenwid  = _winw * (1.0 + _transitioninterp * _screensqueezeamp);
                    var _screenhei  = _winh * (1.0 - _transitioninterp * _screensqueezeamp);
                    var _screeny    = lerp(_winh * 0.5, 0.0, _transitioninterp);
                    var _screentint = make_color_hsv(200, _transitioninterp * 255, _transitioninterp * 255);
                    draw_surface_stretched_ext(global.surfScreen, (_winw -_screenwid) * 0.5, _screeny, _screenwid, _screenhei, _screentint, 1.0);
                }
                else
                {
                    var _screensqueezeamp = 0.4;
                    var _transitioninterp = interp_weight(transitionCtr, transitionTime, 1.5, 2.0);
                    
                    var _screenwid  = _winw * ((1.0 + _screensqueezeamp) - _transitioninterp * _screensqueezeamp);
                    var _screenhei  = _winh * ((1.0 - _screensqueezeamp) + _transitioninterp * _screensqueezeamp);
                    var _screeny    = lerp(_winh, 0.0, _transitioninterp);
                    var _screentint = make_color_hsv(128, _transitioninterp * 255, _transitioninterp * 255);
                    draw_surface_stretched_ext(global.surfScreen, (_winw -_screenwid) * 0.5, _screeny, _screenwid, _screenhei, _screentint, 1.0);
                }
            }
            else
            {
                draw_surface_stretched(global.surfScreen, 0, 0, _winw, _winh);
            }
            
            // draw_surface_stretched(global.surfScreen, 0, 0, _winw, _winh);
        shader_reset();
        
        
        // Draw the resulting surface
        // draw_surface_stretched(_crtsurf, 0, 0, _winw, _winh);
        
        texture_set_interpolation(false);
        
        // Free temp. surfaces
        surface_free(_crtsurf);
    }
    else
    {
        // background
        draw_clear(0);
        
        var _surfw   = surface_get_width(global.surfScreen);
        var _surfh   = surface_get_height(global.surfScreen);
        var _noise   = sprite_get_texture(texBluenoise, 0);
        var _rainbow = sprite_get_texture(texRainbowTable, 0);
        var _shadowmask = sprite_get_texture(texCRTShadowmask, 0);
        // var _shadowmaskwid = 1 / 18;
        
        // First pass : CRT filter
        texture_set_interpolation(true);

        var _screenwid  = _surfw;
        var _screenhei  = _surfh;
        var _screeny    = 0;
        var _screentint = c_white;
        if (transitionIsHappening)
        {
            if (transitionIsFadeout)
            {
                var _screensqueezeamp = 0.8;
                var _transitioninterp = interp_weight(transitionCtr, transitionTime, 2.0, 1.5);
                
                var _screenwid  = _surfw * (1.0 + _transitioninterp * _screensqueezeamp);
                var _screenhei  = _surfh * (1.0 - _transitioninterp * _screensqueezeamp);
                var _screeny    = lerp(_surfh * 0.5, 0.0, _transitioninterp);
                var _screentint = make_color_hsv(200, _transitioninterp * 255, _transitioninterp * 255);
            }
            else
            {
                var _screensqueezeamp = 0.4;
                var _transitioninterp = interp_weight(transitionCtr, transitionTime, 1.5, 2.0);
                
                var _screenwid  = _surfw * ((1.0 + _screensqueezeamp) - _transitioninterp * _screensqueezeamp);
                var _screenhei  = _surfh * ((1.0 - _screensqueezeamp) + _transitioninterp * _screensqueezeamp);
                var _screeny    = lerp(_surfh, 0.0, _transitioninterp);
                var _screentint = make_color_hsv(128, _transitioninterp * 255, _transitioninterp * 255);
            }
        }
        
        var _surfpp_screenw = surface_get_width(global.surfPP_SCREEN);
        var _surfpp_screenh = surface_get_height(global.surfPP_SCREEN);
        surface_set_target(global.surfPP_SCREEN);
        shader_set(shd_postprocessing_screeneffect);
            texture_set_stage(shader_get_sampler_index(shd_postprocessing_screeneffect, "uTexNoise"), _noise);
            texture_set_stage(shader_get_sampler_index(shd_postprocessing_screeneffect, "uTexRainbow"), _rainbow);
            texture_set_stage(shader_get_sampler_index(shd_postprocessing_screeneffect, "uTexShadowmask"), _shadowmask);
            
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_screeneffect, "uTime"), current_time * 0.001);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_screeneffect, "uShadowmaskIntensity"), 0.4);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_screeneffect, "uBrightness"), 0.23);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_screeneffect, "uContrast"), 1.0);
            
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_screeneffect, "uScreenTexelSize"), 1 / _surfw, 1 / _surfh);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_screeneffect, "uNoiseTexelSize"), texture_get_texel_width(_noise), texture_get_texel_height(_noise));
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_screeneffect, "uShadowmaskTexelSize"), texture_get_texel_width(_shadowmask), texture_get_texel_height(_shadowmask));
            
            draw_surface_stretched(global.surfScreen, 0, 0, _surfpp_screenw, _surfpp_screenh);
        shader_reset();
        surface_reset_target();
        
        surface_set_target(global.surfPP_CRT);
        shader_set(shd_postprocessing_camscreen);
            texture_set_stage(shader_get_sampler_index(shd_postprocessing_camscreen, "uTexNoise"), _noise);
            texture_set_stage(shader_get_sampler_index(shd_postprocessing_camscreen, "uTexRainbow"), _rainbow);
            
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_camscreen, "uTime"), current_time * 0.001);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_camscreen, "uScreenTexelSize"), 1 / _surfw, 1 / _surfh);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_camscreen, "uNoiseTexelSize"), texture_get_texel_width(_noise), texture_get_texel_height(_noise));
        
            draw_clear(0);
            draw_surface_stretched_ext(global.surfPP_SCREEN, (_surfw -_screenwid) * 0.5, _screeny, _screenwid, _screenhei, _screentint, 1.0);
        shader_reset();
        surface_reset_target();
        
        // Draw the final surface
        shader_set(shd_postprocessing_effects);
            texture_set_stage(shader_get_sampler_index(shd_postprocessing_effects, "uTexNoise"), _noise);
            texture_set_stage(shader_get_sampler_index(shd_postprocessing_effects, "uTexRainbow"), _rainbow);
            
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_effects, "uTime"), current_time * 0.001);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_effects, "uScreenTexelSize"), 1 / _winw, 1 / _winh);
            shader_set_uniform_f(shader_get_uniform(shd_postprocessing_effects, "uNoiseTexelSize"), texture_get_texel_width(_noise), texture_get_texel_height(_noise));
                
            draw_surface_stretched(global.surfPP_CRT, 0, 0, _winw, _winh);
        shader_reset();
        
        // Disable interpolation
        texture_set_interpolation(false);
    }
}
else
{
    /// draw w/o shaders applied
    // background
    draw_clear(0);
    
    // animate transition
    if (transitionIsHappening)
    {
        if (transitionIsFadeout)
        {
            var _screensqueezeamp = 0.8;
            var _transitioninterp = interp_weight(transitionCtr, transitionTime, 2.0, 1.5);
            // _transitioninterp = 1.0 - _transitioninterp;
            var _screenwid  = _winw * (1.0 + _transitioninterp * _screensqueezeamp);
            var _screenhei  = _winh * (1.0 - _transitioninterp * _screensqueezeamp);
            var _screeny    = lerp(_winh * 0.5, 0.0, _transitioninterp);
            var _screentint = make_color_hsv(200, _transitioninterp * 255, _transitioninterp * 255);
            draw_surface_stretched_ext(global.surfScreen, (_winw -_screenwid) * 0.5, _screeny, _screenwid, _screenhei, _screentint, 1.0);
        }
        else
        {
            var _screensqueezeamp = 0.4;
            var _transitioninterp = interp_weight(transitionCtr, transitionTime, 1.5, 2.0);
            
            var _screenwid  = _winw * ((1.0 + _screensqueezeamp) - _transitioninterp * _screensqueezeamp);
            var _screenhei  = _winh * ((1.0 - _screensqueezeamp) + _transitioninterp * _screensqueezeamp);
            var _screeny    = lerp(_winh, 0.0, _transitioninterp);
            var _screentint = make_color_hsv(128, _transitioninterp * 255, _transitioninterp * 255);
            draw_surface_stretched_ext(global.surfScreen, (_winw -_screenwid) * 0.5, _screeny, _screenwid, _screenhei, _screentint, 1.0);
        }
    }
    else
    {
        draw_surface_stretched(global.surfScreen, 0, 0, _winw, _winh);
    }
}

